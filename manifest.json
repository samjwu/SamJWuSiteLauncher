{
    "manifest_version": 2,
    "name": "SamJWu Site Launcher",
    "description": "Shortcuts to links on samjwu.github.io",
    "version": "1.0.1",
    "version_name": "beta",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
