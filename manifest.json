{
    "manifest_version": 2,
    "name": "samjwu Site Launcher",
    "description": "Shortcuts to samjwu Links",
    "version": "1.0.0",
    "version_name": "pre-release",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/icon16.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}